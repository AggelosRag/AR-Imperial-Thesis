{
    "name": "parabola_tree",
    "n_gpu": 1,

    "arch": {
        "type": "ParabolaArchitecture",
        "args": {}
    },
    "model": {
        "lr": 0.001,
        "weight_decay": 0
    },
    "dataset": {
        "num_classes": 2,
        "num_features": 2,
        "train_size": 200,
        "feature_names": ["x", "y"],
        "concept_names": ["x", "y"],
        "class_names": ["0", "1"],
        "contour_plot": true,
        "use_attribute_imbalance": false
    },
    "data_loader": {
        "type": "get_parabola_dataLoader",
        "args":{
            "data_dir": "./datasets/parabola",
            "type": "SGD",
            "batch_size": 200
        }
    },
    "trainer": {
        "type": "BlackBoxXYTrainer",
        "args": {},
        "epochs": 200000,
        "save_dir": "saved/",
        "save_period": 200000,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 0,
        "tensorboard": true
    },
    "regularisation": {
        "type": "Tree",
        "tree_reg_mode": "Joint",
        "tree_apl_type": "node_count",
        "reg_strength": 5,
        "mse_loss_strength": 5,
        "min_samples_leaf": 1,
        "warm_up_epochs": 30,
        "snapshot_epochs": 5000
    }
}
