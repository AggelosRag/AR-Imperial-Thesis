{
    "name": "cbm_mnist_L2_0.03_joint_alpha=1",
    "n_gpu": 1,

    "arch": {
        "type": "MNISTCBMArchitecture",
        "args": {}
    },
    "model": {
        "lr": 0.001,
        "weight_decay": 0.03,
        "alpha": 1
    },
    "dataset": {
        "type": "JointCBMTrainer",
        "num_classes": 3,
        "num_concepts": 12,
        "train_size": 8859,
        "feature_names": ["thickness_small", "thickness_medium", "thickness_large", "thickness_xlarge",
                         "width_small", "width_medium", "width_large", "width_xlarge",
                         "length_small", "length_medium", "length_large", "length_xlarge"],
        "class_names": ["6", "8", "9"],
        "contour_plot": 0
    },
    "data_loader": {
        "type": "get_mnist_dataLoader",
        "args":{
            "data_dir": "./datasets/MNIST",
            "type": "Full-GD"
        }
    },
    "metrics": ["concept_loss", "target_loss", "correct", "total", "bce_loss_per_concept",
                "APL", "total_loss"],
    "trainer": {
        "type": "JointCBMTrainer",
        "args": {},
        "epochs": 2500,
        "save_dir": "saved/",
        "save_period": 1000,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboard": true
    },
    "regularisation": {
        "type": "L2",
        "min_samples_leaf": 150,
        "snapshot_epochs": 50
    }
}
