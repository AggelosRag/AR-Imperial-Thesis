{
    "name": "final_cbm_mnist_joint_alpha=1",
    "n_gpu": 1,

    "arch": {
        "type": "MNISTCBMTreeArchitecture",
        "args": {}
    },
    "model": {
        "lr": 0.001,
        "weight_decay": 0,
        "alpha": 100
    },
    "dataset": {
        "num_classes": 3,
        "num_concepts": 12,
        "concept_names": ["thickness_small", "thickness_medium", "thickness_large", "thickness_xlarge",
                         "width_small", "width_medium", "width_large", "width_xlarge",
                         "length_small", "length_medium", "length_large", "length_xlarge"],
        "class_names": ["6", "8", "9"],
        "contour_plot": false,
        "use_attribute_imbalance": false
    },
    "data_loader": {
        "type": "get_mnist_dataLoader_original",
        "args":{
            "data_dir": "./datasets/MNIST",
            "type": "SGD",
            "batch_size": 8859
        }
    },
    "trainer": {
        "type": "JointCBMTrainer",
        "args": {},
        "epochs": 200,
        "save_dir": "saved/",
        "save_period": 200,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 0,
        "tensorboard": true
    },
    "regularisation": {
        "type": "Tree",
        "tree_reg_mode": "Joint",
        "tree_apl_type": "node_count",
        "reg_strength": 0.001,
        "mse_loss_strength": 0.001,
        "min_samples_leaf": 150,
        "warm_up_epochs": 30,
        "snapshot_epochs": 10
    }
}
